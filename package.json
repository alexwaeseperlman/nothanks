{
  "name": "nothanks",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server/index.js",
  "scripts": {
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc --project tsconfig.json",
    "build:client": "tsc --project tsconfig.client.json",
    "start": "npm run build && node dist/server/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server/index.ts",
    "bot:example": "ts-node src/bots/exampleBot.ts",
    "bot:smart": "ts-node src/bots/smartBot.ts",
    "demo": "npm run build && node dist/scripts/runBotDemo.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^5.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^24.9.2",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
